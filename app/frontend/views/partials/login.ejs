<div class="container">
    <form name="login" action="./login" method="post">


        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                <div class="form-group form-group-lg"><input type="email" class="form-control" name="email" placeholder="email" required></div>
            </div>

        </div>

        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                <div class="form-group form-group-lg"><input type="password" name="password" class="form-control" placeholder="password" required></div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <input type="submit" value="Login" class="btn btn-default navbar-btn">
            </div>
        </div>



    </form>



</div>
